{
  "name": "customerJoke",
  "version": "1.0.0",
  "main": "server/server.js",
  "scripts": {
    "lint": "eslint .",
    "start-dev": "cross-env NODE_ENV=multienv npm_config_db=dev-mode babel-node src/server/server.js",
    "start": "cross-env NODE_ENV=multienv babel-node src/server/server.js",
    "test": "npm run unit-test && npm run e2e-test",
    "unit-test": "cross-env NODE_ENV=multienv npm_config_db=dev-mode nyc --require babel-core/register --reporter=lcov mocha src/tests/unit/UNIT.js && nyc report",
    "e2e-test": "cross-env NODE_ENV=multienv npm_config_db=dev-mode nyc --require babel-core/register --reporter=lcov mocha src/tests/smoke/SMOKE.js && nyc report",
    "posttest": "npm run lint && nsp check"
  },
  "dependencies": {
    "babel-cli": "6.18.0",
    "babel-core": "6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "compression": "^1.0.3",
    "cors": "^2.5.2",
    "helmet": "^1.3.0",
    "loopback": "^2.22.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^4.1.0",
    "loopback-connector-mongodb": "^1.17.0",
    "loopback-datasource-juggler": "^2.39.0",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^1.0.1",
    "uuid": "^3.0.1",
    "node-rest-client":"3.1.0",
    "http-errors":"1.6.1",
    "cross-env": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "babel-plugin-rewire": "1.0.0-rc-3",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-eslint": "*",
    "eslint": "^3.11.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-import": "~2.1.0",
    "nsp": "^2.1.0",
    "mocha": "^2.4.5",
    "expect": "^1.16.0",
    "supertest": "^2.0.1",
    "nyc": "10.2.0"
  },
  "nyc": {
    "exclude": [
      "**/helper.js",
      "**/server.js",
      "**/datasources.multienv.js"
    ]
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "customerJoke"
}
